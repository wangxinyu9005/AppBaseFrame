<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.xinbo.app.baseframe.MainActivity">

    <include layout="@layout/app_base_base_title_toolbar" />

    <TextView
        android:id="@+id/test_show_dialog"
        android:layout_width="wrap_content"
        android:text="dialog演示"
        android:textSize="30sp"
        android:textColor="@color/app_base_white"
        android:background="@color/app_base_black"
        android:layout_below="@+id/base_title_layout"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/test_get_data"
        android:layout_width="wrap_content"
        android:text="网络请求测试"
        android:layout_marginTop="10dp"
        android:textSize="30sp"
        android:textColor="@color/app_base_white"
        android:background="@color/app_base_black"
        android:layout_below="@+id/base_title_layout"
        android:layout_height="wrap_content" />

</LinearLayout>